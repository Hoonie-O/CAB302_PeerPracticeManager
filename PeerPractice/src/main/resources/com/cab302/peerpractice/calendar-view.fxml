<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cab302.peerpractice.Controllers.CalendarController">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <ToggleButton fx:id="sessionViewButton" text="Study Sessions" />
            <ToggleButton fx:id="availabilityViewButton" text="Availability" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="prevButton" onAction="#onPreviousMonth" text="&lt;" />
            <Label fx:id="monthYearLabel" text="Month Year">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="nextButton" onAction="#onNextMonth" text="&gt;" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <GridPane fx:id="calendarGrid" alignment="CENTER" gridLinesVisible="false" hgap="1.0" vgap="1.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="backToMenuButton" onAction="#onBackToMenu" text="Back to Menu" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox><?xml version="1.0" encoding="UTF-8"?>

